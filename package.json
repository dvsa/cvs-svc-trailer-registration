{
  "name": "cvs-svc-trailer-registration",
  "version": "1.0.0",
  "description": "",
  "main": "src/handler.ts",
  "engines": {
    "node": "18.*",
    "npm": "8.*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all security-checks audit prettier lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm-run-all build test test-i"
    }
  },
  "scripts": {
    "start": "cross-env API_VERSION=${npm_package_version} NODE_ENV=local serverless offline start",
    "dev": "cross-env SLS_DEBUG=* concurrently \"npm start\" \"npm run test:unit:watch\"",
    "test:unit": "cross-env NODE_ENV=test SERVICE=${npm_package_name} API_VERSION=${npm_package_version} jest tests/unit",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:unit:coverage": "cross-env npm run test:unit -- --coverage",
    "test-i": "BRANCH=local npm run test:integration -- --globalSetup='./scripts/setup.ts' --globalTeardown='./scripts/teardown.ts'",
    "test:integration": "cross-env NODE_ENV=test jest tests/integration",
    "test:integration:watch": "npm run test:integration -- --watch",
    "test:integration:coverage": "cross-env npm run test:integration -- --coverage",
    "test": "npm-run-all test:unit:coverage",
    "audit": "npm audit",
    "lint:analyse": "eslint . --ext js,ts --fix",
    "lint:report": "npm run lint:analyse -- -f json -o reports/eslint/eslint-report.json",
    "lint": "npm-run-all lint:*",
    "prettier": "prettier --write ./**/*.{js,ts}",
    "security-checks": "git secrets --scan && git log -p | scanrepo",
    "clean": "rimraf ./.build ./.artifact ./.serverless ./.webpack ./*.zip",
    "compile": "tsc",
    "build:dev": "cross-env SERVICE=${npm_package_name} NODE_ENV=local serverless webpack",
    "build:production": "source config/.env.production && cross-env SERVICE=${npm_package_name} NODE_ENV=production webpack -c ./scripts/webpack.config.build.js --env=production",
    "build": "npm-run-all clean build:dev",
    "sonar-scanner": "sonar-scanner",
    "package:create": "cross-env SERVICE=${npm_package_name} NODE_ENV=local sls package --package .build",
    "package:prepare": "mv ./.build/app.zip ./.build/build.zip",
    "package:move": "mv ./.build/build.zip ./${ZIP_NAME}.zip",
    "package": "npm-run-all clean package:*",
    "release": "semantic-release",
    "prerelease": "semantic-release --dry-run",
    "dependencies:check": "ncu",
    "tools-setup": "node_modules/.bin/sls dynamodb install"
  },
  "dependencies": {
    "aws-lambda": "1.0.7",
    "aws-sdk": "2.1421.0",
    "dotenv": "16.3.1",
    "express": "4.19.2",
    "jest-express": "1.12.0",
    "joi": "17.9.2",
    "serverless-http": "3.2.0",
    "source-map-support": "0.5.21"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-conventional": "17.6.7",
    "@dvsa/eslint-config-ts": "3.0.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.0.4",
    "@semantic-release/npm": "10.0.4",
    "@serverless/typescript": "3.30.1",
    "@types/aws-lambda": "8.10.119",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.3",
    "@types/node": "20.4.4",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "clean-webpack-plugin": "4.0.0",
    "commitlint-plugin-function-rules": "2.0.2",
    "concurrently": "8.2.0",
    "cross-env": "7.0.3",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-prettier": "5.0.0",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "gulp": "4.0.2",
    "husky": "8.0.3",
    "jest": "29.6.1",
    "json-serverless": "1.6.16",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.0",
    "rimraf": "5.0.1",
    "semantic-release": "21.0.7",
    "serverless": "3.33.0",
    "serverless-dotenv-plugin": "6.0.0",
    "serverless-dynamodb": "0.2.42",
    "serverless-offline": "12.0.4",
    "serverless-webpack": "5.12.0",
    "sonar-scanner": "3.1.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "webpack": "5.88.2",
    "webpack-node-externals": "3.0.0"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dvsa/cvs-svc-trailer-registration.git/issues"
  },
  "homepage": "https://github.com/dvsa/cvs-svc-trailer-registration.git#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/dvsa/cvs-svc-trailer-registration.git"
  },
  "keywords": [
    "DVLA",
    "DVSA",
    "CVS",
    "TRL",
    "VRM",
    "MOT"
  ]
}
